<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <!-- Required zalo app id -->

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.App_System_Management_Visitors_For_Old_Aparment"
        tools:targetApi="31">
        <activity
            android:name=".crud.DeleteAccountActivity"
            android:exported="false" />
        <activity
            android:name=".crud.EditAccountActivity"
            android:exported="false" />
        <activity
            android:name=".crud.AddNewAccountActivity"
            android:exported="false" />
        <activity
            android:name=".crud.DeleteApartmentActivity"
            android:exported="false" />
        <activity
            android:name=".list.ListFeedbackActivity"
            android:exported="false" />
        <activity
            android:name=".list.ManageListApartmentActivity"
            android:exported="false" />
        <activity
            android:name=".list.ManageListVisitorActivity"
            android:exported="false" />
        <activity
            android:name=".list.ManageListAccountActivity"
            android:exported="false" />
        <activity
            android:name=".list.ListApartmentActivity"
            android:exported="false">
            <intent-filter>
                <action android:name="android.intent.action.CALL" />
            </intent-filter>
        </activity>
        <activity
            android:name=".list.ListVisitorActivity"
            android:exported="false" />
        <activity
            android:name=".dashboard.DashboardAdminActivity"
            android:exported="false" />
        <activity
            android:name=".dashboard.DashboardActivity"
            android:exported="false" />
        <activity
            android:name=".authentication.PinCodeActivity"
            android:exported="false" />
        <activity
            android:name=".crud.EditApartmentActivity"
            android:exported="false" />
        <activity
            android:name=".crud.AddNewApartmentActivity"
            android:exported="false" />
        <activity
            android:name=".main_activity.FeedbackActivity"
            android:exported="false" />
        <activity
            android:name=".main_activity.RatingActivity"
            android:exported="false" />
        <activity
            android:name=".main_activity.RegisterFormActivity"
            android:exported="false" />
        <activity
            android:name=".authentication.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.zing.zalo.zalosdk.appID"
            android:value="@string/appID" />
        <activity
            android:name=".main_activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>